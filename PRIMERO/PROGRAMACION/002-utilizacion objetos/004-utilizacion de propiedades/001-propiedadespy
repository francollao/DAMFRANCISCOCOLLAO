class Gato:
    def __int___(self): #definir la plantilla de un gato que esta vacia
        self.altura = None
        self.edad = None
        self,peso = None
        self.nombre = None
       
    def maulla(self,numero): #a√±adimos un parametro
        cadena= " Miau " * numero 
        print(cadena)
    def estufera(self):
        print("ffffffffffff")


gato1 = Gato()
#print("El nombre del gato 1 es :",gato1.nombre)
gato1.nombre = "micifu"
print("El nombre del gato 1 es :",gato1.nombre)
